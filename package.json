  },
  "homepage": "https://github.com/oleesir/wayfarer-api#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
